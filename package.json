{
  "name": "iqoption-blitz-bot",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "server": "bun run src/server/index.ts",
    "macd": "bun run src/bot/run-macd.ts",
    "cli": "bun run src/cli/cli.ts",
    "db:generate": "bunx drizzle-kit generate",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio",
    "lint:time": "! grep -rn 'Date\\.now()' src/bot/agents/ --include='*.ts' | grep -v 'node_modules' | grep -v '// lint:allow-date-now'"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/yargs": "^17.0.35",
    "better-sqlite3": "^12.6.2",
    "drizzle-kit": "^0.31.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.9",
    "onnxruntime-node": "^1.18.0",
    "yargs": "^18.0.0",
    "zod": "^4.3.6"
  }
}
